{
  "tasks": {
    "dev": "deno run -A -q dev.ts",
    "start": "deno run -A server.tsx",
    "firestore-emulator": "npx firebase-tools@10.9.2 emulators:start",
    "test": "deno test -A --unstable"
  },
  "fmt": {
    "files": {
      "exclude": [
        "dist",
        "vendor"
      ]
    }
  },
  "lint": {
    "rules": {
      "exclude": [
        "import-prefix-missing"
      ]
    },
    "files": {
      "exclude": [
        "dist",
        "vendor"
      ]
    }
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns",
      "deno.unstable"
    ],
    "types": [
      "https://deno.land/x/aleph@1.0.0-alpha.85/types.d.ts",
      "./types.d.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.1.0"
  },
  "importMap": "./import_map.json"
}
